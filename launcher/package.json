{
  "name": "desktop-launcher",
  "version": "1.0.0",
  "description": "Native launcher for the Desktop application using Bun WebView with embedded backend",
  "main": "src/main.ts",
  "scripts": {
    "dev": "bun run src/main.ts",
    "build": "bun run scripts/build.ts",
    "build:windows": "bun run scripts/build.ts windows",
    "build:linux": "bun run scripts/build.ts linux",
    "build:macos": "bun run scripts/build.ts macos",
    "build:macos-arm": "bun run scripts/build.ts macos-arm"
  },
  "dependencies": {
    "webview-bun": "^2.4.0"
  },
  "devDependencies": {
    "@types/bun": "latest"
  }
}
